{
  "name": "Videology",
  "version": "1.0",
  "description": "BGN Hackathon",
  "manifest_version": 2,
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js","popup.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/V_icon16.png",
      "32": "icons/V_icon32.png",
      "48": "icons/V_icon48.png",
      "128": "icons/V_icon128.png"
    }
  },

  "icons": {
    "16": "icons/V_icon16.png",
    "32": "icons/V_icon32.png",
    "48": "icons/V_icon48.png",
    "128": "icons/V_icon128.png"
  }

}